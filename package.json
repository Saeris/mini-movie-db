    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.0.9",
    "apollo-link-retry": "^2.2.3",
    "apollo-link-state": "0.4.1",
    "apollo-server-hapi": "^1.3.4",
    "aws-lambda": "^0.1.2",
    "babel-preset-env": "^1.7.0",
    "date-fns": "1.29.0",
    "formik": "0.11.11",
    "good": "^8.1.1",
    "good-winston": "^4.0.0",
    "graphql": "0.13.2",
    "graphql-deduplicator": "2.0.1",
