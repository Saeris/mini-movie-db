{
  "name": "mini-movie-db",
  "version": "1.1.0",
  "author": "Drake Costa <drake@saeris.io> (http://saeris.github.io/)",
  "description": "A simple clone of The Movie DB built with Preact + Apollo.",
  "private": false,
  "license": "MIT",
  "keywords": [
    "preact",
    "the movie db",
    "apollo",
    "graphql",
    "saeris.io"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel start:api start:client playground",
    "start": "npm-run-all --parallel start:api start:client",
    "start:api": "serverless offline start --dontPrintOutput",
    "start:client": "preact watch --template public/template.html",
    "build": "npm-run-all --parallel build:client",
    "build:client": "preact build --no-prerender --template public/template.html",
    "deploy:dev": "cross-env NODE_ENV=development serverless --stage=dev deploy",
    "deploy:prod": "cross-env NODE_ENV=production serverless --stage=production deploy",
    "test": "cross-env BABEL_ENV=test nyc ava --verbose --color --tap",
    "test:watch": "cross-env BABEL_ENV=test ava --watch",
    "test:coverage": "cross-env BABEL_ENV=test nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:report": "cross-env BABEL_ENV=test nyc report --color",
    "playground": "graphql playground",
    "precommit": "lint-staged",
    "prepublish": "snyk protect"
  },
  "homepage": "https://codesandbox.io/s/github/Saeris/mini-movie-db",
  "main": "src/index.js",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/runtime": "^7.0.0-beta.42",
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "apollo-boost": "^0.1.4",
    "apollo-server-hapi": "1.1.2",
    "apollo-server-lambda": "^1.3.3",
    "aws-lambda": "^0.1.2",
    "babel-preset-env": "1.6.1",
    "date-fns": "1.29.0",
    "formik": "0.11.11",
    "good": "7.3.0",
    "good-winston": "^4.0.0",
    "graphql": "0.13.2",
    "graphql-import": "^0.5.0",
    "graphql-playground": "^1.3.8",
    "graphql-playground-middleware-lambda": "1.4.4",
    "graphql-tag": "2.8.0",
    "graphql-tools": "^2.5.0",
    "hapi": "16.6.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.8",
    "node-vibrant": "3.0.0",
    "polished": "1.9.2",
    "preact": "^8.2.7",
    "preact-cli": "2.2.1",
    "preact-compat": "^3.18.0",
    "react-apollo": "^2.1.1",
    "react-clamp-lines": "^1.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "4.2.2",
    "react-spinners": "0.2.6",
    "react-youtube": "7.5.0",
    "regenerator-runtime": "0.11.1",
    "styled-components": "3.2.3",
    "winston": "^2.3.1",
    "yup": "0.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "ava": "^1.0.0-beta.3",
    "aws-sdk": "^2.125.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "browser-env": "^3.2.5",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.0.5",
    "del-cli": "^1.1.0",
    "dotenv": "^5.0.0",
    "eslint": "^4.8.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-graphql": "^1.4.0-1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "esm": "^3.0.12",
    "graphql-cli": "^2.14.2",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.2.1",
    "prettier": "^1.7.0",
    "sass-loader": "^6.0.7",
    "serverless": "^1.26.0",
    "serverless-offline": "^3.15.3",
    "serverless-webpack": "^5.0.0",
    "snyk": "^1.42.6",
    "stylelint": "^9.1.1",
    "stylelint-no-unsupported-browser-features": "^2.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.0",
    "tap-nyan": "^1.1.0",
    "webpack": "^4.3.0",
    "webpack-node-externals": "^1.6.0"
  },
  "greenkeeper": {
    "ignore": [
      "hapi",
      "good",
      "apollo-server-hapi"
    ]
  },
  "snyk": true,
  "ava": {
    "files": [
      "!api/**/*.{js,jsx}",
      "!src/**/*.{js,jsx}",
      "!lib/**/*.{js,jsx}",
      "!**/webpack.config.js",
      "api/**/__TEST__/*.spec.js",
      "src/**/__TEST__/*.spec.js",
      "lib/**/__TEST__/*.spec.js"
    ],
    "sources": [
      "api/**/*.{js,jsx}",
      "src/**/*.{js,jsx}",
      "lib/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": "tap-nyan",
    "powerAssert": true,
    "require": [
      "esm",
      "./register.js"
    ]
  }
}
