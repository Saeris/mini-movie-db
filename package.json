    "preact-compat": "^3.18.0",
    "preact-content-loader": "^1.0.0",
    "react-apollo": "^2.1.1",
    "react-clamp-lines": "^1.1.0",
    "react-intersection-observer": "^3.0.3",
    "react-lazy-images": "^0.9.0",
    "react-router": "^4.2.0",
